@model int 

<td style="width: 150px">
    <div class="btn-group">
        <a asp-action="Edit" class="btn" asp-route-id="@Model">
            <i class="fa fa-edit"></i>
        </a>
        <a asp-action="Details" class="btn" asp-route-id="@Model">
            <i class="fa fa-list-alt"></i>
        </a>
        <a asp-action="Delete" class="btn danger" asp-route-id="@Model">
            <i class="fa fa-trash"></i>
        </a>
    </div>
</td> 